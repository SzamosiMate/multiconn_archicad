{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Returns the detailed fill attributes identified by their GUIDs.",
  "definitions": {
    "command_parameters": {
      "type": "object",
      "properties": {
        "attributeIds": {
          "$ref": "APIAttributeTypes.json#/definitions/AttributeIds"
        }
      },
      "additionalProperties": false,
      "required": [
        "attributeIds"
      ]
    },
    "response_parameters": {
      "type": "object",
      "properties": {
        "attributes": {
          "$ref": "APIAttributeTypes.json#/definitions/FillAttributes"
        }
      },
      "additionalProperties": false,
      "required": [
        "attributes"
      ]
    },
    "examples": [
      {
        "command_example": {
          "command": "API.GetFillAttributes",
