        "heightStretchable": {
          "type": "boolean",
          "description": "Defines whether the profile's height can be increased beyond its default value or not."
        },
        "hasCoreSkin": {
          "type": "boolean",
          "description": "Defines whether the profile has a core skin or not."
        },
        "profileModifiers": {
          "$ref": "#/definitions/ProfileModifierList"
        }
