      "type": "object",
      "description": "A profile attribute.",
      "properties": {
        "attributeId": {
          "$ref": "#/definitions/AttributeId"
        },
        "name": {
          "$ref": "#/definitions/AttributeName"
        },
        "useWith": {
          "$ref": "APITypes.json#/definitions/ElementTypes",
          "description": "The list of element types for which this profile is available."
        },
        "width": {
          "type": "number",
          "description": "The default width (horizontal size) of the profile."
        },
        "height": {
          "type": "number",
          "description": "The default height (vertical size) of the profile."
        },
        "minimumWidth": {
          "type": "number",
          "description": "The minimum width (horizontal size) of the profile."
