        "emissionAttenuation": {
          "type": "integer",
          "description": "The emission attenuation of the surface attribute.",
          "minimum": 0,
          "maximum": 65535
        },
        "surfaceColor": {
          "$ref": "APITypes.json#/definitions/RGBColor",
          "description": "The surface color of the surface attribute."
        },
        "specularColor": {
          "$ref": "APITypes.json#/definitions/RGBColor",
          "description": "The specular color of the surface attribute."
        },
        "emissionColor": {
          "$ref": "APITypes.json#/definitions/RGBColor",
          "description": "The emission color of the surface attribute."
        },
        "fillId": {
          "$ref": "#/definitions/AttributeIdOrError",
          "description": "The identifier of the surface attribute's fill component."
        },
        "transparency": {
          "type": "integer",
          "description": "The transparency of the surface attribute.",
          "minimum": 0,
          "maximum": 100
        },
        "shine": {
          "type": "integer",
          "description": "The shininess of the surface attribute.",
          "minimum": 0,
          "maximum": 10000
        },
        "texture": {
          "$ref": "#/definitions/Texture",
          "description": "The texture of the surface attribute."
        }
      },
      "additionalProperties": false,
      "required": [
        "attributeId",
        "name",
        "materialType",
        "ambientReflection",
        "diffuseReflection",
        "specularReflection",
        "transparencyAttenuation",
        "emissionAttenuation",
        "surfaceColor",
        "specularColor",
        "emissionColor",
        "fillId",
        "transparency",
        "shine"
