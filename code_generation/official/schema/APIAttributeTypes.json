      "type": "string",
      "description": "The type of an attribute.",
      "enum": [
        "BuildingMaterial",
        "Composite",
        "Fill",
        "Layer",
        "LayerCombination",
        "Line",
        "PenTable",
        "Profile",
        "Surface",
        "ZoneCategory"
      ]
    },
    "AttributeName": {
      "type": "string",
      "description": "The name of an attribute.",
      "minLength": 1
    },
    "AttributeFolderName": {
      "type": "string",
