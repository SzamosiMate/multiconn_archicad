    },
    "FillTypeId": {
      "type": "string",
      "description": "The filling type of a fill attribute.",
      "enum": [
        "Vector",
        "Symbol",
        "Solid",
        "Empty",
        "LinearGradient",
        "RadialGradient",
        "Image"
      ]
    },
