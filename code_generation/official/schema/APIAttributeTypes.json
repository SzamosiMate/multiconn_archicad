        "name": {
          "$ref": "#/definitions/AttributeName"
        },
        "subType": {
          "$ref": "#/definitions/FillTypeId"
        },
        "pattern": {
          "$ref": "#/definitions/FillPattern"
        },
        "appearanceType": {
          "$ref": "#/definitions/AppearanceType"
        }
      },
      "additionalProperties": false,
      "required": [
        "attributeId",
        "name",
        "subType",
        "pattern",
        "appearanceType"
      ]
