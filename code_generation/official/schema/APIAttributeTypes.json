    "AttributeFolderRenameParameters": {
      "type": "object",
      "description": "Used to rename an attribute folder. The folder is identified by it's Id.",
      "properties": {
        "attributeFolderId": {
          "$ref": "#/definitions/AttributeFolderId"
        },
        "newName": {
          "$ref": "#/definitions/AttributeFolderName"
        }
