{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Returns the identifiers of selected elements in the current plan.",
  "definitions": {
    "command_parameters": {
      "type": "object",
      "properties": {
        "onlyEditable": {
          "type": "boolean",
          "description": "Optional parameter that defines whether the selection list should include only the editable elements or all of them. The default value is FALSE"
        },
        "onlySupportedTypes": {
          "type": "boolean",
          "description": "Optional parameter. When it is set to true, only elements with types that are supported by any other JSON API command will be returned."
        }
      },
      "additionalProperties": false
    },
    "response_parameters": {
      "type": "object",
      "properties": {
        "elements": {
          "$ref": "APITypes.json#/definitions/Elements"
        }
      },
      "additionalProperties": false,
      "required": [
        "elements"
      ]
    },
    "examples": [
      {
        "command_example": {
          "command": "API.GetSelectedElements",
          "parameters": {
            "onlyEditable": true,
            "onlySupportedTypes": true
          }
        },
        "response_example": {
          "succeeded": true,
          "result": {
            "elements": [
              {
                "elementId": {
                  "guid": "0F7943AC-48FA-C74E-8ACE-D1CB0DB0DBB7"
                }
              },
              {
                "elementId": {
                  "guid": "63BA003E-4F4D-AA4A-8FBC-57379F2CDB98"
                }
              },
              {
                "elementId": {
                  "guid": "6C7F82AF-A362-4CFD-AF06-3CA1668D92F1"
                }
              },
              {
                "elementId": {
                  "guid": "65237B0F-73C3-4DB3-9E94-3E1931F84AA0"
                }
              }
            ]
          }
        }
      }
    ],
    "see_also": [
      "Element Information"
    ]
  }
}