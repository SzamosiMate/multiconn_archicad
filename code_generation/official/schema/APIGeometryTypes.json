{
  "$schema": "http://json-schema.org/draft-04/schema",
  "definitions": {
    "Point2D": {
      "type": "object",
      "description": "Coordinates of a 2D point",
      "properties": {
        "x": {
          "type": "number",
          "description": "X coordinate of 2D point"
        },
        "y": {
          "type": "number",
          "description": "Y coordinate of 2D point"
        }
      },
      "additionalProperties": false,
      "required": [
        "x",
        "y"
      ]
    }
  }
}