{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Accesses the version information from the running Archicad.",
  "definitions": {
    "command_parameters": {
      "type": "object",
      "properties": {
      },
      "additionalProperties": false
    },
    "response_parameters": {
      "type": "object",
      "properties": {
        "version": {
          "type": "integer",
          "description": "The version of the running Archicad."
        },
        "buildNumber": {
          "type": "integer",
          "description": "The build number of the running Archicad."
        },
        "languageCode": {
          "type": "string",
          "description": "The language code of the running Archicad."
        }
      },
      "additionalProperties": false,
      "required": [
        "version",
        "buildNumber",
        "languageCode"
      ]
    },
    "examples": [
      {
        "command_example": {
          "command": "API.GetProductInfo"
        },
        "response_example": {
          "succeeded": true,
          "result": {
            "version": 24,
            "buildNumber": 3003,
            "languageCode": "INT"
          }
        }
      }
    ]
  }
}
