        "name": {
          "type": "string",
          "description": "The localized name of the property."
        },
        "description": {
          "type": "string",
          "description": "The description of the property."
        },
        "isEditable": {
          "type": "boolean",
          "description": "Defines whether the property is editable or not."
        },
        "type": {
          "type": "string",
          "description": "Defines the type of the property's value."
        },
        "possibleEnumValues": {
          "$ref": "#/definitions/PossibleEnumValues",
          "description": "The possible enum values of the property when the property type is enumeration."
        },
        "defaultValue": {
          "$ref": "#/definitions/PropertyDefaultValue"
        }
      },
      "additionalProperties": false,
      "required": [
        "group",
        "name",
        "description",
        "isEditable",
        "type"
      ]
    },
