{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Returns the detailed folder structure for the attributes of a given type. If the path is not given, the root folder will be returned",
  "definitions": {
    "command_parameters": {
      "type": "object",
      "properties": {
        "attributeType": {
          "$ref": "APIAttributeTypes.json#/definitions/AttributeType"
        },
        "path": {
          "$ref": "APIAttributeTypes.json#/definitions/AttributeFolderPath"
        }
      },
      "additionalProperties": false,
      "required": [
        "attributeType"
      ]
    },
    "response_parameters": {
      "type": "object",
      "properties": {
        "attributeFolder": {
          "$ref": "APIAttributeTypes.json#/definitions/AttributeFolderStructure"
        }
      },
      "additionalProperties": false,
      "required": [
        "attributeFolder"
      ]
    },
